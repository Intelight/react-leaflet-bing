{
  "name": "react-leaflet-bing",
  "version": "4.3.2",
  "description": "Bing layer as React compoment for Leaflet",
  "main": "dist/react-leaflet-bing.js",
  "types": "types/react-leaflet-bing.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "build:lib": "npm run compile:lib",
    "build:dist": "webpack src/index.js dist/react-leaflet-bing.js & webpack -p src/index.js dist/react-leaflet-bing.min.js",
    "build": "webpack",
    "example": "webpack-dev-server --config ./example/webpack.config.js",
    "lint": "eslint ./src",
    "release:major": "npm version major && git push --follow-tags",
    "release:minor": "npm version minor && git push --follow-tags",
    "release:patch": "npm version patch && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Charmatzis/react-leaflet-bing.git"
  },
  "keywords": [
    "react",
    "leaflet",
    "bing"
  ],
  "author": "Christos Charmatzis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Charmatzis/react-leaflet-bing/issues"
  },
  "homepage": "https://github.com/Charmatzis/react-leaflet-bing#readme",
  "peerDependencies": {
    "leaflet": "^1.3.0",
    "prop-types": "^15.5.0",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-leaflet": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/react-leaflet": "^2.5.2",
    "babel-loader": "^8.1.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "leaflet": "^1.3.4",
    "lodash.isequal": "^4.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-leaflet": "^2.0.1",
    "rollup": "^2.26.5",
    "rollup-plugin-babel": "^4.4.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
